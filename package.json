{
  "name": "multi-checkout-action",
  "version": "1.0.1",
  "description": "A GitHub Action to checkout multiple additional repositories",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "standard",
    "build": "cp node_modules/checkout/dist/index.js dist/checkout.js && cp node_modules/checkout/dist/problem-matcher.json dist/problem-matcher.json"
  },
  "dependencies": {
    "checkout": "github:actions/checkout#v3.3.0"
  },
  "devDependencies": {
    "standard": "^16.0.1"
  },
  "repository": "vweevers/multi-checkout-action",
  "engines": {
    "node": ">=12"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "keywords": [
    "action",
    "checkout",
    "clone",
    "multi",
    "repository"
  ]
}
